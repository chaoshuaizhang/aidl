<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="net.shopin.myaidlclient">

    <!-- 使用service权限 -->
    <!-- 说明：要用带有权限的组件，就要在这里声明下 -->
    <uses-permission android:name="net.shopin.myaidlserver.book_service_perr"/>
    <uses-permission android:name="net.shopin.myaidlserver.perrrrr"/>
    <uses-permission android:name="net.shopin.server.content_provider"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".ContentProviderActivity">
        </activity>
        <activity android:name=".BinderPoolActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>